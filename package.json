{
  "name": "imKey-manager",
  "version": "0.0.13",
  "author": "imkey <weiqiang@imkey.im>",
  "description": "imKey-manager @2020",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "mocha test/e2e",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix",
    "release": "node .electron-vue/build.js && electron-builder"
  },
  "build": {
    "productName": "imkey-manager",
    "appId": "com.imkey.imkey-manager",
    "copyright": "Copyright © 2020 imkey",
    "directories": {
      "output": "build"
    },
    "publish": {
      "provider": "github",
      "repo": "imkey-manager",
      "owner": "fengweiqiang-coder",
      "releaseType": "release"
    },
    "releaseInfo": {
      "releaseNotes": "1.解决了更新失败的问题 \n 2.解决了绑定失败的问题"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "extraResources": [
        {
          "from": "./connector.dylib",
          "to": "../"
        }
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "requestedExecutionLevel": "highestAvailable",
      "target": "nsis",
      "extraResources": [
        {
          "from": "./connector.dll",
          "to": "../"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true
    },
    "linux": {
      "icon": "build/icons/icon.icns"
    }
  },
  "dependencies": {
    "@sentry/electron": "^1.3.0",
    "body-parser": "^1.19.0",
    "electron-updater": "^4.3.1",
    "element-ui": "^2.13.1",
    "express": "^4.17.1",
    "ffi": "github:lxe/node-ffi#node-12",
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.15",
    "lodash-id": "^0.14.0",
    "ref": "github:lxe/ref#node-12",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.17.7",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "@sentry/cli": "^1.53.0",
    "@vue/devtools": "^5.3.3",
    "ajv": "^6.10.0",
    "@babel/core": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-loader": "^8.1.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "bufferutil": "^4.0.1",
    "cfonts": "^2.4.2",
    "chalk": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "del": "^4.1.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.13",
    "electron-builder": "^22.6.1",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-download": "^4.1.1",
    "electron-rebuild": "^1.11.0",
    "file-loader": "^3.0.1",
    "google-protobuf": "3.11.4",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "utf-8-validate": "^5.0.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "inject-loader": "^4.0.1",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.2",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "require-dir": "^1.2.0",
    "spectron": "^11.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "should": "^13.2.3",
    "mocha": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consenlabs/imkey-manager.git"
  },
  "bugs": {
    "url": "https://github.com/consenlabs/imkey-manager/issues"
  },
  "homepage": "https://github.com/consenlabs/imkey-manager#readme"
}
