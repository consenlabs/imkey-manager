{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["@babel/env", {
          "targets": { "node": 12 }
        }],
      ],
      "plugins": ["istanbul"]
    },
    "main": {
      "presets": [
        ["@babel/env", {
          "targets": { "node": 12 }
        }],
      ]
    },
    "renderer": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }],
      ]
    },
    "web": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }],
      ]
    }
  },
  "plugins": ["@babel/plugin-transform-runtime"]
}
